{
  "name": "cdk-boilerplate",
  "version": "0.1.0",
  "bin": {
    "cdk-boilerplate": "infra/app.js"
  },
  "scripts": {
    "build": "tsc --build --clean",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk-synth-dev": "cdk synth -c config=develop --profile aws_profile",
    "cdk-synth-qa": "cdk synth -c config=qa --profile aws_profile",
    "cdk-synth-prod": "cdk synth -c config=prod --profile aws_profile",
    "cdk-diff-dev": "npm run build && cdk diff \"*\" -c config=develop --profile aws_profile",
    "cdk-diff-qa": "npm run build && cdk diff \"*\" -c config=qa --profile aws_profile",
    "cdk-diff-prod": "tsc && cdk diff \"*\" -c config=prod --profile aws_profile",
    "cdk-deploy-dev": "tsc && cdk deploy \"*\" -c config=dev --profile aws_profile --progress=events",
    "cdk-deploy-qa": "tsc && cdk deploy \"*\" -c config=qa --profile aws_profile --progress=events",
    "cdk-deploy-prod": "tsc && cdk deploy \"*\" -c config=prod --profile aws_profile --progress=events",
    "cdk-deploy-dev2": "tsc && cdk deploy \"*\" -c config=dev --profile aws_profile --require-approval=never --progress=events",
    "cdk-deploy-prod2": "tsc && cdk deploy \"*\" -c config=prod --profile aws_profile --require-approval=never --progress=events"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.12.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.12.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
